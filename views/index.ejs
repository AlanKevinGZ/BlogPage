<%- include("partials/header.ejs") %>

<section class="card_section mt-5">
    <div class="container">
        <h2 class="text-center">All Posts</h2>
        <div class="cards_col">
            <% posts.forEach(post => { %>
                <div class="card_prop">
                    <div class="img_card">
                        <img src="img/test.png" alt="">
                    </div>
                    <div class="info_card">
                        <h2><%= post.title %></h2>
                        <p><%= post.comments %></p>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</section>

<%- include("partials/footers.ejs") %>
